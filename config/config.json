{
    "server": {
        "accessLog": "/tmp/access.log",
        "workers": 10,
        "maxSockets": 100,
        "deadBackendTTL": 30,
        "tcpTimeout": 30,
        "retryOnError": 3,
        "deadBackendOn500": true,
        "httpKeepAlive": false
    },
    "https": {
        "port": 443,
        "bind": [ "0.0.0.0" ],
        "key": "/ssl/ssl.key",
        "cert": "/ssl/ssl.crt",
        "ca": [ "/ssl/substrait.pem" ],
        "requestCert": true,
        "rejectUnauthorized": false
    },
    "http": {
        "port": 80,
        "bind": [ "0.0.0.0" ]
    },
    "driver": "etcd://172.17.42.1:4001/#/hipache/"
}
